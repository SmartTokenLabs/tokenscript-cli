<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ts:token xmlns:ts="http://tokenscript.org/2022/09/tokenscript"
		  xmlns:xml="http://www.w3.org/XML/1998/namespace"
		  xsi:schemaLocation="http://tokenscript.org/2022/09/tokenscript http://tokenscript.org/2022/09/tokenscript.xsd"
		  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		  xmlns:ethereum="urn:ethereum:constantinople"
		  xmlns:xhmtl="http://www.w3.org/1999/xhtml"
		  name="Test2">
	<ts:label>
		<ts:plurals xml:lang="en">
			<ts:string quantity="one">Test2 Token</ts:string>
			<ts:string quantity="other">Test2 Tokens</ts:string>
		</ts:plurals>
	</ts:label>
	<ts:contract interface="erc721" name="TestToken">
		<ts:address network="5">0x07d6AE261994e0024377c2306178123582aC62d6</ts:address>
	</ts:contract>
	<ts:attestation name="MyAttestation" type="eas">
		<ts:meta>
			<ts:title>My Attestation</ts:title>
			<ts:image>https://somesite.com/someimage.png</ts:image>
			<ts:description>A really good attestation</ts:description>
		</ts:meta>
		<ts:key></ts:key> <!-- Public key in base64 PEM -->
		<!--
			Event ID is the same as devconId/conferenceId but renamed to be more generic.
			It can be used to define multiple "attestation collections" for the same public key and schema ID.
			If omitted, all attestations with this schemaUID and pubkey are considered the same collection
		-->
		<ts:eventId>devcon7</ts:eventId>
		<!--
			This field defines the inner attestation fields used to construct a unique ID for the attestation.
			In this way, duplicate attestations are not imported and an attestation can be revoked and reissued.
			When the user imports the attestation again it replaces the old one.
			If omitted, the EAS attestation UID is used.
		-->
		<ts:idFields>eventId.ticketId</ts:idFields>
		<ts:eas schemaUID="0x7f6fb09beb1886d0b223e9f15242961198dd360021b2c9f75ac879c0f786cafd" />
	</ts:attestation>
	<ts:origins>
		<!-- Define the contract which holds the token that the user will use -->
		<ts:ethereum contract="TestToken"/>
		<ts:attestation name="MyAttestation"/>
	</ts:origins>
	<ts:cards>
		<ts:viewContent name="common" xmlns="http://www.w3.org/1999/xhtml">
			<ts:include type="html" src="./dist/index.html"/>
			<ts:include type="css" src="./src/styles.css"/>
		</ts:viewContent>
		<ts:card type="action" name="Mint">
			<!-- this action is of the model confirm-back.
			window.onConfirm is called if user hit "confirm";
			window.close() causes the back button to be pressed.
			-->
			<ts:label>
				<ts:string xml:lang="en">Mint</ts:string>
			</ts:label>

			<ts:attribute name="toAddress">
				<ts:type>
					<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
				</ts:type>
				<ts:origins>
					<ts:user-entry as="address"/>
				</ts:origins>
			</ts:attribute>

			<ts:transaction>
				<ethereum:transaction as="uint" contract="TestToken" function="safeMint">
					<ts:data>
						<ts:address ref="toAddress" />
						<ts:string>https://smart-token-labs-demo-server.mypinata.cloud/ipfs/QmXdnWNa2CaRCUa4jMTirmkVJkygr2LtnccSuDY3yXrvVm/bayc_1013.json</ts:string>
					</ts:data>
				</ethereum:transaction>
			</ts:transaction>

			<ts:view xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" urlFragment="mint-action-1">
				<ts:viewContent name="common"/>
				<!--<ts:include type="html" src="./dist/index.html"/>-->
			</ts:view>
		</ts:card>
		<ts:card type="action" name="Mint2">
			<!-- this action is of the model confirm-back.
			window.onConfirm is called if user hit "confirm";
			window.close() causes the back button to be pressed.
			-->
			<ts:label>
				<ts:string xml:lang="en">Mint 2</ts:string>
			</ts:label>
			<ts:attribute name="toAddress">
				<ts:type>
					<ts:syntax>1.3.6.1.4.1.1466.115.121.1.15</ts:syntax>
				</ts:type>
				<ts:origins>
					<ts:user-entry as="address"/>
				</ts:origins>
			</ts:attribute>
			<ts:transaction>
				<ethereum:transaction as="uint" contract="TestToken" function="safeMint">
					<ts:data>
						<ts:address ref="toAddress" />
						<ts:string>https://smart-token-labs-demo-server.mypinata.cloud/ipfs/QmXdnWNa2CaRCUa4jMTirmkVJkygr2LtnccSuDY3yXrvVm/bayc_1013.json</ts:string>
					</ts:data>
				</ethereum:transaction>
			</ts:transaction>
			<ts:view xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" urlFragment="mint-action-2">
				<ts:viewContent name="common"/>
				<!--<ts:include type="html" src="./dist/index.html"/>-->
			</ts:view>
		</ts:card>
	</ts:cards>

</ts:token>
