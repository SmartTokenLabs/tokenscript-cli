{
  "name": "@tokenscript/cli",
  "version": "0.0.0",
  "description": "A tool for managing tokenscript projects",
  "author": "Michael Wallace @micwallace",
  "bin": {
    "tokenscript": "./bin/run"
  },
  "homepage": "https://github.com/TokenScript/tokenscript-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "TokenScript/tokenscript-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.13.10",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@xmldom/xmldom": "^0.8.6",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.4",
    "jsdom": "^20.0.0",
    "libxmljs2-xsd": "^0.30.1",
    "replace-in-file": "^6.3.5",
    "xmldsigjs": "^2.5.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/filesystem": "^0.0.32",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^9.0.1",
    "@types/jsdom": "^20.0.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.59",
    "@types/w3c-xmlserializer": "^2.0.2",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "node-gyp": "^9.3.0",
    "oclif": "^3",
    "patch-package": "^6.5.0",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "tokenscript",
    "dirname": "tokenscript",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && npm run copy-files",
    "copy-files": "cp -R src/create/templates dist/create/ && cp -R src/schema dist/",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "postinstall": "patch-package"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/TokenScript/tokenscript-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
